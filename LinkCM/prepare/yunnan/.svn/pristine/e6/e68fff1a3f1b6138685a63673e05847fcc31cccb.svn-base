.timepicker-container {
    font-size: 12px;
    line-height: 20px;
    position: fixed;
    z-index: -1;
    top: 0;
    left: 0;
    width: 210px;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    background-color: #fff;
    direction: ltr !important;
    -ms-touch-action: none;
    touch-action: none;
    -webkit-tap-highlight-color: transparent;
    -webkit-touch-callout: none;
}

.timepicker-container:before,
.timepicker-container:after {
    position: absolute;
    display: block;
    width: 0;
    height: 0;
    content: ' ';
    border: 5px solid transparent;
}

.timepicker-dropdown {
    position: absolute;
    z-index: 1;
    -webkit-box-sizing: content-box;
    -moz-box-sizing: content-box;
    box-sizing: content-box;
    border: 1px solid #ccc;
    -webkit-box-shadow: 0 3px 6px #ccc;
    box-shadow: 0 3px 6px #ccc;
}

.timepicker-inline {
    position: static;
}

.timepicker-top-left,
.timepicker-top-right {
    border-top-color: #39f;
}

.timepicker-top-left:before,
.timepicker-top-left:after,
.timepicker-top-right:before,
.timepicker-top-right:after {
    top: -5px;
    left: 10px;
    border-top: 0;
}

.timepicker-top-left:before,
.timepicker-top-right:before {
    border-bottom-color: #39f;
}

.timepicker-top-left:after,
.timepicker-top-right:after {
    top: -4px;
    border-bottom-color: #fff;
}

.timepicker-bottom-left,
.timepicker-bottom-right {
    border-bottom-color: #39f;
}

.timepicker-bottom-left:before,
.timepicker-bottom-left:after,
.timepicker-bottom-right:before,
.timepicker-bottom-right:after {
    bottom: -5px;
    left: 10px;
    border-bottom: 0;
}

.timepicker-bottom-left:before,
.timepicker-bottom-right:before {
    border-top-color: #39f;
}

.timepicker-bottom-left:after,
.timepicker-bottom-right:after {
    bottom: -4px;
    border-top-color: #fff;
}

.timepicker-top-right:before,
.timepicker-top-right:after,
.timepicker-bottom-right:before,
.timepicker-bottom-right:after {
    right: 10px;
    left: auto;
}
.timepicker-panel{
	height:204px;
}
.timepicker-panel > ul:before,
.timepicker-panel > ul:after {
    display: table;
    content: ' ';
}

.timepicker-panel > ul:after {
    clear: both;
}

.timepicker-panel > ul {
    width: 100%;
    margin: 0;
    padding: 0;
}

.timepicker-panel > ul > li {
    float: left;
    width: 30px;
    height: 20px;
    margin: 0;
    padding: 0;
    list-style: none;
    cursor: pointer;
    text-align: center;
    background-color: #fff;
}

.timepicker-panel > ul > li:hover {
    background-color: #eee;
}

.timepicker-panel > ul > li.muted,
.timepicker-panel > ul > li.muted:hover {
    color: #999;
}

.timepicker-panel > ul.days > li.picked,
.timepicker-panel > ul.days > li.picked:hover {
    background: rgb(85, 139, 193);
    color: #FFF;
}

.timepicker-panel > ul.years > li.picked,
.timepicker-panel > ul.months > li.picked,
.timepicker-panel > ul.years > li.picked:hover,
.timepicker-panel > ul.months > li.picked:hover {
    background: rgb(85, 139, 193);
    color: #FFF;
}

.timepicker-panel > ul > li.disabled,
.timepicker-panel > ul > li.disabled:hover {
    cursor: default;
    color: #ccc;
    background-color: #fff;
}

.timepicker-panel > ul > li[data-view='years prev'],
.timepicker-panel > ul > li[data-view='year prev'],
.timepicker-panel > ul > li[data-view='month prev'],
.timepicker-panel > ul > li[data-view='years next'],
.timepicker-panel > ul > li[data-view='year next'],
.timepicker-panel > ul > li[data-view='month next'],
.timepicker-panel > ul > li[data-view='next'] {
    font-size: 18px;
}

.timepicker-panel > ul > li[data-view='years current'],
.timepicker-panel > ul > li[data-view='year current'],
.timepicker-panel > ul > li[data-view='month current'] {
    width: 150px;
}

.timepicker-panel > ul[data-view='years'] > li,
.timepicker-panel > ul[data-view='months'] > li {
    line-height: 52.5px;
    width: 52.5px;
    height: 52.5px;
}

.timepicker-panel > ul[data-view='week'] > li,
.timepicker-panel > ul[data-view='week'] > li:hover {
    cursor: default;
    background-color: #fff;
}

.timepicker-hide {
    display: none;
}

.timepicker-panel > ul > li[data-view='hour prev'],
.timepicker-panel > ul > li[data-view='hour next'] {
    font-size: 18px;
    width: 45px;
}

.timepicker-panel > ul > li.colon {
    width: 15px;
    color: #000;
}

.timepicker-panel > ul > li[class="colon disabled"]:hover {
    color: #000;
}

.timepicker-panel > ul > li[data-view='days current'] {
    width: 210px;
}

.timepicker-panel > ul.times > li[data-view='hour'], .timepicker-panel > ul.times > li[data-view='hour picked'] {
    width: 35px;
    height:40px;
    line-height: 40px;
}

.timepicker-panel > ul.times > li[data-view='min'],
.timepicker-panel > ul.times > li[data-view='second'],
.timepicker-panel > ul.times > li[data-view='min picked'],
.timepicker-panel > ul.times > li[data-view='second picked'] {
    width: 35px;
    height:40px;
    line-height: 40px;
}

.timepicker-panel > ul.times > li.picked,
.timepicker-panel > ul.times > li.picked:hover {
    background: rgb(85, 139, 193);
    width: 35px;
    height:40px;
    line-height: 40px;
    color: #FFF;
}

.timepicker-panel > ul {
    border-bottom: 1px solid #eee;
}

.timepicker-panel > ul[data-view="week"] {
    border-bottom: 0;
}

.timepicker-panel > ul.confirm > li {
    width: 105px;
    height: 20px;
}

.timepicker-panel > ul.confirm > li:nth-child(1) {
    width: 104px;
    border-right: 1px solid #eee;
}